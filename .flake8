[flake8]
max-line-length = 120
inline-quotes = '"'
extend-ignore = E203, W503, E501, E226
exclude =
    .tox
    .git
    */migrations/*
    */static/CACHE/*
    docs
    node_modules
    migrations
    __pycache__
    manage.py
    settings.py
    venv
    .venv
    ./venv
    venv/
    .venv/
    __init__
    __init__.py
